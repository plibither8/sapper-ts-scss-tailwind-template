{
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": ["error", { "args": "none" }],
    "no-console": "off"
  },
  "env": {
    "es2021": true,
    "browser": true,
    "node": true
  },
  "plugins": ["prettier", "svelte3"]
}
